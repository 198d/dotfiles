#! /bin/bash
function install_files {
  for file in "$@"
  do
    echo -n "Installing $file..."
    if [ -d $file ]; then
      rsync -a $file/ $HOME/.$file
    else
      rsync $file $HOME/.$file
    fi
    echo "done."
  done
}


install_files bashrc bashrc.d gitignore tmux.conf vimrc vim pyenv gitconfig
